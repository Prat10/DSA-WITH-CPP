class Node{
public:
    Node* prev;
    int data;
    Node* next;
 
    Node()
    { 
        prev = NULL; 
        data = 0;
        next = NULL;
    }
    
    Node(int value)
    { 
        prev = NULL; 
        data = value;
        next = NULL;
    }
};*/

class Solution {
  public:
    Node* constructDLL(vector<int>& arr) {
        // code here
        Node* head=NULL;
        Node* tail=NULL;
        for(int i=0;i<arr.size();i++){
            Node* newnode=new Node(arr[i]);
             if(i==0){
                 head=newnode;
             }
             else {
                 newnode->prev=tail;
                 tail->next=newnode;
             }
             tail=newnode;
        }
        return head;
    }
};
